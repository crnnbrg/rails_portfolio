<%= render "layouts/errors", :object => @project %>

  <%= form_for @project do |f| %>

    <%= f.label :link %>
    <%= f.text_field :link %>
    <br>
    <%= f.label :title %>
    <%= f.text_field :title %>
    <br>
    <%= f.label :image %>
    <%= f.text_field :image %>
    <br>
    <%= f.select(:user_id) do %>
    <% User.all.each do |usr| %>
      <%= content_tag(:option, usr.name, value: usr.id) %>
    <% end %>
  <% end %>
  <%= f.submit %>
  <% end %>
