<%= render "layouts/errors", :object => @project %>

  <%= form_for @project do |f| %>
    <%= f.label :title %>
    <%= f.text_field :title %>
    <br>
    <%= f.label :image %>
    <%= f.text_field :image %>
      <br>
    <%= f.label :link %>
    <%= f.text_field :link %>
      <br>
    <%= f.label :user_id %>
    <%= f.text_field :user_id %>
    <%= f.submit %>
  <% end %>
