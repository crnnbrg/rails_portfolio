<%= render "layouts/errors", :object => @project %>

<%= form_for @project, remote: true do |f| %>

<%= f.label :link %>
<%= f.text_field :link %>

<%= f.label :title %>
<%= f.text_field :title %>

<%= f.label :description %>
<%= f.text_field :description %>

<%= f.label :skill %>
<%= f.text_field :skill %>

<%= f.file_field :picture %>
<br>
<%= f.submit %>
<% end %>
